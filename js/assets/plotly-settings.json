{
  "layout": {
    "title": {
      "font": {
        "family": "Arial",
        "size": 25
      },
      "x": "0"
    },
    "width": 850,
    "height": 530,
    "showlegend": false,
  	"images": [{
  		"source": "./image/human.png",
